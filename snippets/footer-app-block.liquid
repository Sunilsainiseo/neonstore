        <script>
            window.neonCustomiser = window.neonCustomiser || {};
            window.neonCustomiser.moneyFormat = "{% raw %}${{amount}}{% endraw %}";                
            window.neonCustomiser.rtl = false;
            window.neonCustomiser.scrollOrder = false;          
        </script>
        <style>
            @media screen and (max-width: 1024px) {
                #neon-customiser-app .frame-preview:not(.review-frame):not(.npc-frame-preview--stacked),
                #neon-customiser-app .customiser-loading {
                    min-height: calc((100vh - 0px) * 0.6);
                }
                #neon-customiser-app > div {
                    min-height: calc(100vh - 0px);
                }
                #neon-customiser-app .sidebar-loading {
                    height: calc((100vh - 0px) * 0.4);
                }
            }
            @media screen and (min-width: 1025px) {
                #neon-customiser-app > div {
                    height: calc(100vh - 0px);
                }
            }
        </style>
        <!-- END app snippet -->
    </div>
