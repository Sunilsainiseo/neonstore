{% if canonical_url contains '/products/custom-design' %}
    <div id="shopify-block-10004392781266402770" class="shopify-block shopify-app-block">
        <!-- BEGIN app snippet: neon-customiser-modules -->
        <script>
            var link = document.createElement('link');
            link.rel = 'modulepreload';
            link.href = "https://cdn.shopify.com/extensions/68b887ef-dea6-4c09-9322-70c47265fe0f/neon-sign-customiser-83/assets/neon-sign-customiser-d7937d10.js";
            link.as = 'script';
            document.head.appendChild(link);
        </script>
        <script src="https://cdn.shopify.com/extensions/68b887ef-dea6-4c09-9322-70c47265fe0f/neon-sign-customiser-83/assets/neon-sign-customiser-d7937d10.js" type="module"></script>
        <!-- END app snippet -->
        <!-- BEGIN app snippet: neon-customiser-vars -->
