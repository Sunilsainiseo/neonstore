
  <div class="product-icons row">
    
    {% for block in section.blocks %}
       <div class="col-md-3">
         <div class="p-ixon-div-ins">
        <img class="iconeimg" src="{{ block.settings.icon_imagess | img_url:'master' }}">

         <h5>{{block.settings.icon_title}}</h5>
         <p>{{block.settings.icon_subtitle}}</p>
       </div>
         </div>
    
    {% endfor %}

    </div>



