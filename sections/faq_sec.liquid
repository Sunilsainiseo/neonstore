<div class="new-faq-sec image-custom-faqs">
    <div class="container">
    <div class="title-bar custom-font">
      <h2 class="main-heading" style="font-weight: normal; font-size: 34px !important; margin-bottom: 10px;">{{section.settings.sec_title}}</h2>
    </div>

    <div class="accordion_row-main">
      <div class="accordion_col-6-main">
        <div class="accordion_container">
